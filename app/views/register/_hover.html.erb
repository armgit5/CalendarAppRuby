

<!-- / javascript -->
<%= javascript_tag do %>

$(document).ready(function() {
 $.ionSound({
            sounds: [
                "click"
            ],
            path: "http://www.fromtexttospeech.com/texttospeech_output_files/0851407001394869628/",
            path: "http://k007.kiwi6.com/hotlink/5c7cg3lngg/",
            multiPlay: true,
            volume: "1.0"
        });
        
    $( "a" ).hover(
      function() {
        $.ionSound.play("click");
      }, function() {
        $.ionSound.stop("click");
      }
    );
    
});  
<% end %>