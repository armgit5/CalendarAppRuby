<h1>Delete Page</h1>

<%= link_to("Index", {:action => 'index'}) %>


<%= form_tag(:action => 'destroy', :id => @schedule.id) do %>
  <table class="list"  summary="Schedule list">
    <tr class="header">
      <th>Project</th>
      <th>Date</th>
      <th>Customer</th>

    </tr>

    <tr>
      <td><%= @schedule.project %></td>
      <td><%= @schedule.date %></td>
      <td><%= @schedule.customer %></td>
    </tr>

  </table>

<%= submit_tag('Delete Schedule') %>
  
  
<% end %>
  